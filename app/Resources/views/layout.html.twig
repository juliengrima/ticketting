{% extends 'base.html.twig' %}


{% block header %}

    <!-- Wrapper -->
    <div id="wrapper">

        <!-- Header -->
        <header id="header">
            <div class="logo">
                <span class="icon fa-diamond"></span>
            </div>
            <div class="content">
                <div class="inner">
                    <h1>Ticketting Applet</h1>
                    <p>An application to make tickets<br />
                        and screen dates in calendar</p>
                </div>
            </div>
            <nav>
                <ul>
                    <li><a href="{{ path('ticket_index') }}">Tickets</a></li>
                    <li><a href="#">Work</a></li>
                    <li><a href="#">About</a></li>
                    <li><a href="#">Contact</a></li>
                    <li><a href="#">Elements</a></li>
                </ul>
            </nav>
        </header>

{% endblock %}

{% block footer %}

        <!-- Footer -->
        <footer id="footer">
            <p class="copyright">&copy; Julien GRIMA <a href="https://html5up.net">HTML5 UP</a>.</p>
        </footer>

    </div>

<!-- BG -->
    <div id="bg"></div>

{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('assets/js/jquery.min.js') }}"></script>
    <script src="{{ asset('assets/js/browser.min.js') }}"></script>
    <script src="{{ asset('assets/js/breakpoints.min.js') }}"></script>
    <script src="{{ asset('assets/js/util.js') }}"></script>
    <script src="{{ asset('assets/js/main.js') }}"></script>
{% endblock %}